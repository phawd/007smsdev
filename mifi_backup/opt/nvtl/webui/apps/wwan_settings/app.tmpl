<tmpl_if isHidden>
	<div class="notification app error">
		<tmpl_var _("webui", "webui_app_hidden_text")>
	</div>
<tmpl_else>
	<script type="text/javascript">
		/* <![CDATA[ */
		var isSubmit = false;
		var showNetworkDialog = false;
		var showAPNDialog = false;
		var dataConnected = false;
		var connectionPreferenceDefaultIndex;
		var msg;
		var gForm;
		var previousConnectionPreference;
		$(function()
		{
			$("#NetworksConnectionPreference").focus(function(e){       
                		previousConnectionPreference = $(this).val();                                                                                                                 
            		}); 

			$(document).ajaxSuccess(function(e, xhr, settings){
				if ((xhr.responseText) && (settings.dataType == "json")) {
					try {
						var obj = jQuery.parseJSON(xhr.responseText || "null");
						if ( typeof obj.statusData.statusBarConnectionState !== "undefined") {
							// Verizon can go into a dormant state but still remain, technically, connected.
							if ( (obj.statusData.statusBarConnectionState == $.uiEnums.WANSTATE_CONNECTED)
								|| (obj.statusData.statusBarConnectionState == $.uiEnums.WANSTATE_DORMANT) ) {
								dataConnected = true;
							} else if (gForm) {
								gForm.submit();
							}
						}
					} catch(e) {}
				}
			});
			if ("<tmpl_var reboot>" === "true") {
				window.location.href = "<tmpl_udf GET_URI('restarting')>";
			}

			<tmpl_if NetworksConnectionPreferenceShow>
				connectionPreferenceDefaultIndex = $("#NetworksConnectionPreference").prop("selectedIndex");
			</tmpl_if>

			<tmpl_if ShowMNSInGlobalTech>
				if(("<tmpl_var NetworksPreferredTechnology>" === "<tmpl_var MnsTechMode>") || ("<tmpl_var NetworksPreferredTechnology>" === "<tmpl_var AutomaticTechMode>") ){
			<tmpl_else>
				if(("<tmpl_var NetworksPreferredTechnology>" === "<tmpl_var MnsTechMode>") ){
			</tmpl_if>
				<tmpl_if NetworksSelectMNSShow>
					<tmpl_unless NetworksSelectMNSLocked>
						$("#NetworksSelectMNS").prop("disabled", false);
					</tmpl_unless>
				</tmpl_if>
			} else {
				$("#NetworksSelectMNS").prop("disabled", true);
			}
			
			//show Mns Page
			<tmpl_if NetworksSelectMNSShow>
				<tmpl_unless NetworksSelectMNSLocked>
					$("#NetworksSelectMNS").on("click", function(){
						<tmpl_if showMNSAsPopUp>
							var title = '<tmpl_var _("webui", "menu_adv_mns_text")>'
							var url = <tmpl_udf GET_URI('mns')>

							modalPopup.loadPage(
								title,
								url,
								900, 700, null,function() 
								{
									location.replace('<tmpl_udf GET_URI("wwan_settings")>') 
								}
							);
						<tmpl_else>
							window.location.href = "<tmpl_udf GET_URI('mns')>";
						</tmpl_if>
					});
				</tmpl_unless>
			</tmpl_if>
			
		<tmpl_if ShowAdvancedSettingsToggleButton>
			$("#advancedSettingsToggle").on("click", function(){
				$("#advancedSettings").slideToggle();
				<tmpl_unless apnProfileSettings>
				if ($("#advancedSettingsToggle").html() === '<tmpl_var _("webui", "wwan_settings_advanced_settings_hide")>') {
					$("#advancedSettingsToggle").html('<tmpl_var _("webui", "wwan_settings_advanced_settings_show")>');
				} else {
					$("#advancedSettingsToggle").html('<tmpl_var _("webui", "wwan_settings_advanced_settings_hide")>');
				}
				</tmpl_unless>
			});
		</tmpl_if>
			<tmpl_if NetworksPreferredTechnologyShow>
				<tmpl_unless NetworksPreferredTechnologyLocked>
					$("#NetworksPreferredTechnology").on("change", function(e){
						showNetworkDialog = dataConnected;
						var preferredTechnology = $("#NetworksPreferredTechnology").val();
						
						if("<tmpl_var mnsIsInUse>" === "1" && preferredTechnology !== "<tmpl_var MnsTechMode>") {
							showWarning("<tmpl_var _('webui', 'wwan_settings_preferred_network_change_confirm_text')>"); 
						}
					});
				</tmpl_unless>
			</tmpl_if>
			<tmpl_unless isLocked>
				<tmpl_if showDisconnectConfirmation>
					$("#verizonGlobalForm").on("submit", submitTasks);
				</tmpl_if>
			</tmpl_unless>

			<tmpl_if NetworksSelectMNSShow>
				<tmpl_unless NetworksSelectMNSLocked>
					<tmpl_unless NetworksOtherAPNShow>
						<tmpl_if mnsUsed>
							$("#NetworksSelectMNS").html('<tmpl_var _("webui", "wwan_settings_scan_for_different_network_btn")>');
							$("#NetworksSelectMNS").css("margin-left", "0px");
						</tmpl_if>
					</tmpl_unless>
				</tmpl_unless>
			</tmpl_if>

			<tmpl_if NetworksRevertToAutomaticShow>
				<tmpl_unless NetworksRevertToAutomaticLocked>
					$("#NetworksRevertToAutomatic").on("click", function(){
						$("#NetworksRevertToAutomatic").hide();
						$.ajax({
							url : "<tmpl_udf GET_URI('wwan_settings_revert_mns')>",
							type : "POST",
							dataType : "json",
							data : { gSecureToken : "<tmpl_var gSecureToken>" },
							success : function(data){
								window.location.reload();
							},
							error : function(xhr, textStatus, errorThrown){
								$("#NetworksRevertToAutomatic").show();
							}
						});
					});
				</tmpl_unless>
			</tmpl_if>

			<tmpl_if NetworksFourGLTEAPNShow>
				<tmpl_unless NetworksFourGLTEAPNLocked>
					<tmpl_unless NetworksOtherAPNShow>
						<tmpl_unless isSimReady> 
							$("#NetworksFourGLTEAPN").val("");
							$("#NetworksFourGLTEAPN").attr('disabled','disabled');
						</tmpl_unless>
					<tmpl_else>
						$("#NetworksFourGLTEAPN").on("blur", function(e){
							showAPNDialog = dataConnected;
						});
					</tmpl_unless>
				</tmpl_unless>
			</tmpl_if>
			<tmpl_if NetworksRestoreDefaultAPNShow>
				<tmpl_unless NetworksRestoreDefaultAPNLocked>
					$("#NetworksRestoreDefaultApn").on("click", function(){
						$("#NetworksFourGLTEAPN").val("<tmpl_var DefaultAPN>");
					});
				</tmpl_unless>
			</tmpl_if> 

			<tmpl_if NetworksRestoreBothAPNsToDefaultShow>
				<tmpl_unless NetworksRestoreBothAPNsToDefaultLocked>
					$("#NetworksRestoreBothAPNsToDefault").on("click", function(){
						$("#NetworksFourGLTEAPN").val("<tmpl_var DefaultAPN>");
						$("#NetworksOtherAPN").val("<tmpl_var CustomDefaultAPN>");
					});
				</tmpl_unless>
			</tmpl_if> 

			// Initialize our fancy toggle checkboxes.
			if (jQuery.fn.slideButton) {
				<tmpl_if NetworksCellularDataEnableShow>
					$("#NetworksCellularDataEnable").slideButton({
						change : function($input){
							if($input.is(":checked") === false ) {
								showConfirmation($input.is(":checked"), $input);
								$("#verizonGlobalForm").hide();
							} else {
								processCellularData($input.is(":checked"), $input);
								$("#verizonGlobalForm").show();
							}
						}
					});
				</tmpl_if>
			}

			<tmpl_if NetworksConnectionPreferenceShow>
				<tmpl_unless NetworksConnectionPreferenceLocked>
					$("#NetworksConnectionPreference").on("change", connectionPreferenceChange );

				</tmpl_unless>
			</tmpl_if>
		});

		function connectionPreferenceChange(){
				var value = parseInt($(this).val());
				showNetworkDialog = dataConnected;
				if (value == 1) {
					msg = '<p><tmpl_var _("webui", "wwan_settings_auto_connect_changed_text")></p>';
				} else if (value == 2) {
					msg = '<p><tmpl_var _("webui", "wwan_settings_auto_connect_not_roaming_changed_text")></p>';
				} else {
					previousConnectionPreference = parseInt($(this).val());
					return true;
				}
				makeModal(msg, false);
			}
	
		function makeModal(msg, doSubmit)
		{
			var okStr = (doSubmit) ? '<tmpl_var _("webui", "wwan_settings_disconnect_button_text")>' : '<tmpl_var _("webui", "webui_modal_confirm_button")>';
			modalPopup.createWindow('<tmpl_var _("webui", "webui_modal_title")>',
				msg, 530, "auto",
				[{
					text : '<tmpl_var _("webui", "webui_modal_cancel_button")>',
					className : "cancel",
					trigger : function() {             
                        			$("#NetworksConnectionPreference").val(previousConnectionPreference);
                        			if (jQuery.uniform) $.uniform.update("#NetworksConnectionPreference");
					}

				}, {
					text : okStr,
					className : "primary",
					trigger : function(){
						if (doSubmit) {
							isSubmit = true;
							$("#verizonGlobalForm").submit();
						}
					}
				}], null,
				function(){
					if (!doSubmit) resetConnectionPreference();
				}
			);
		}

		function resetConnectionPreference()
		{
			$("#NetworksConnectionPreference").prop("selectedIndex", connectionPreferenceDefaultIndex);
			if (jQuery.uniform) $.uniform.update("#NetworksConnectionPreference");
		}

		function submitTasks(e)
			{
				e.preventDefault();
				var theForm = e.target;
				if (isSubmit) {
					disconnectThenSubmit(theForm);
				} else {
					if (showNetworkDialog || showAPNDialog) {
						msg = '<p><tmpl_var _("webui", "wwan_vzw_global_save_changes_confirmation_text")></p>';
						makeModal(msg, true);
					} else {
						disconnectThenSubmit(theForm);
					}
				}
			};

			function disconnectThenSubmit(theForm)
			{
				// waitDialog function from header.tmpl.
				if (dataConnected && (typeof waitDialog !== "undefined") && $.isFunction(waitDialog)) {
					// We need to lock down the UI with an overlay while disconnecting.
					waitDialog();
					$.get("/srv/disconnect").done(function(){
						// Manually set the connect button state and connection status.
						$("#appsHomeNetworkStatusCol").html('<tmpl_var _("webui", "status_bar_disconnecting_text")>');
						$("#connectDisconnect").html('<tmpl_var _("webui", "apps_home_disconnect_button_text")>').prop("disabled", true);
						// A small delay is needed here. Disconnecting can get stuck in pending state.
						// Submitting the form prematurely could cancel the disconnect request.
						setTimeout(function(){
							gForm = theForm;
						}, 500);
					});
				} else {
					theForm.submit();
				}
			}

		function showConfirmation(bool, $cellularElem) {
		        modalPopup.createWindow('<tmpl_var _("webui", "wwan_modal_turn_off_mobile_network_title")>', "<p><tmpl_var _('webui', 'wwan_settings_cellular_off_confirmation_text')></p>", 430, 250, [{
		                text : '<tmpl_var _("webui", "webui_modal_cancel_button")>',
		                className : "cancel",
		                trigger : function () {
		                    undoChange($cellularElem);
		                }
		        }, {
		                text : '<tmpl_var _("webui", "wwan_modal_turn_off_confirm_button")>',
		                className : "primary",
		                trigger : function() {
		                        processCellularData(bool, $cellularElem)
		                }
		        }], null, function(){
				undoChange($cellularElem);
		        });
		    return false;
		}

		function showWarning(text) {            
		    modalPopup.createWindow('<tmpl_var _("webui", "webui_app_warning_title")>', '<p>'+text+'</p>', 430, 250, [{
		        text : '<tmpl_var _("webui", "webui_modal_ok_button")>',
		        className : "primary",
		        trigger : null
		    }], null, null);
		}

		function undoChange($el)
		{
			if (jQuery.fn.slideButton) $el.slideButton("undo");
			if($el.is(":checked") === true){
				$("#verizonGlobalForm").show();
			} else {
				$("#verizonGlobalForm").hide();
			}
		}

		function processCellularData(bool, $cellularElem)
		{
			if (jQuery.fn.slideButton) $cellularElem.slideButton("addLoader");
			var url = (bool) ? "<tmpl_udf GET_URI('wwan_settings_enable_cellular_data')>" : "<tmpl_udf GET_URI('wwan_settings_disable_cellular_data')>";
			$.ajax({
				url : url,
				type : "POST",
				data : {
					gSecureToken : "<tmpl_var gSecureToken>"
				},
				dataType : "json",
				success : function(data, textStatus, xhr){
					// If no network connected, success event still occurs.
					// We need to implicitly see an xhr.status greater than 0 to know we've actually
					// made contact.
					if ((xhr.readyState == 4 ) && (xhr.status == 0)) {
						undoChange($cellularElem);
					}
				},
				error : function(xhr, textStatus, errorThrown){
					undoChange($cellularElem);
				},
				complete : function(xhr, textStatus){
					if (jQuery.fn.slideButton) $cellularElem.slideButton("removeLoader");
				}
			});
		}
		/* ]]> */
	</script>

	<tmpl_if isLocked>
		<div class="notification app error">
			<tmpl_var _("webui", "webui_app_locked_text")>
		</div>
	</tmpl_if>

	<tmpl_if errorCount>
		<div class="notification app error">
			<div class="notification_inner">
				<p>
					<tmpl_var _("webui", "webui_save_code_failed_text")>
				</p>
			</div>
		</div>
	</tmpl_if>

	<tmpl_if gPostSuccess>
		<div class="notification app success">
			<div class="notification_inner">
				<p>
					<tmpl_var _("webui", "webui_save_code_success_text")>
				</p>
			</div>
		</div>
	</tmpl_if>

	<tmpl_if showHeaderText>
		<p class="note"><tmpl_var _("webui", "wwan_settings_header_note_text")></p>
	</tmpl_if>

	<tmpl_if NetworksCellularDataEnableShow>
		<tmpl_if showToolTip >
			<h3 class="sectionHead"><tmpl_var _("webui", "wwan_settings_cellular_data_label")></h3>
		</tmpl_if>
		<div class="toggleSettingContainer scrimp">
			<div class="toggleSettingContainerInner">
				<div class="desc">
					<tmpl_if showToolTip >
						<div class="col insignia">
							<div class="tooltip tooltip-help" title="<tmpl_var _('webui', 'wwan_settings_cellular_data_help_text')>">
								<tmpl_var _("webui", "webui_inline_help_icon")>
							</div>
						</div>
						<div class="col motto">
							<tmpl_var _("webui", "wwan_settings_cellular_data_text")>
						</div>
					<tmpl_else>
						<label for="cellularDataCheckbox"><tmpl_var _("webui", "wwan_settings_cellular_data_label")></label>
						<p>
							<tmpl_var _("webui", "wwan_settings_cellular_data_help_text")>
						</p>
					</tmpl_if>
				</div>
				<div class="toggle">
					<input class="toggleSwitchCheckbox" type="checkbox" id="NetworksCellularDataEnable" name="NetworksCellularDataEnable"<tmpl_if IN_SET(cellularData, "on")> checked="checked"</tmpl_if><tmpl_if isLocked> disabled="disabled"</tmpl_if><tmpl_if NetworksCellularDataEnableLocked> disabled="disabled"</tmpl_if>/>
				</div>
			</div><!--/.toggleSettingContainerInner-->
		</div><!--/.toggleSettingContainer-->
	</tmpl_if>

	<form id="verizonGlobalForm" name="verizonGlobalForm" method="POST" action="<tmpl_udf GET_URI('wwan_settings')>"<tmpl_if IN_SET(cellularData, "off")> style="display:none;"</tmpl_if>>
		<h3 class="sectionHead"><tmpl_var _("webui", "wwan_settings_settings_section_header_text")></h3>
		<tmpl_if showNetworkSettingHelpText><p><tmpl_var _("webui", "wwan_settings_settings_section_help_text")></p></tmpl_if>
		<fieldset id="verizonGlobal" class="first">
			<div class="table">
				<tmpl_if NetworksConnectionPreferenceShow>
					<div class="row clearfix">
						<div class="col label">
							<h4><label for="NetworksConnectionPreference"> <tmpl_var _("webui", "wwan_settings_connection_preference_label")></label></h4>
						</div>
						<div class="col input">
							<select name="NetworksConnectionPreference" id="NetworksConnectionPreference"<tmpl_if isLocked> disabled="disabled"</tmpl_if><tmpl_if NetworksConnectionPreferenceLocked> disabled="disabled"</tmpl_if>>
								<tmpl_include "connection_preference_options.tmpl">
							</select>
						</div>
						<div class="col help" id="NetworksConnectionPreferenceHelp">
							<tmpl_var _("webui", "wwan_settings_connection_preference_help_text")>
						</div>
					</div>
				</tmpl_if>

				<tmpl_if NetworksPreferredTechnologyShow>
					<div class="row clearfix">
						<div class="col label">
							<h4>
								<label for="NetworksPreferredTechnology" > <tmpl_var _("webui", "wwan_settings_preferred_technology_label")></label>
							</h4>
						</div>
						<div class="col input">
							<div id="NetworksPreferredTechnologyOptionsHolder">
								<tmpl_if showNetworkTechOptions>
									<select name="NetworksPreferredTechnology" id="NetworksPreferredTechnology"<tmpl_if isLocked> disabled="disabled"</tmpl_if><tmpl_if NetworksPreferredTechnologyLocked> disabled="disabled"</tmpl_if> >
										<tmpl_foreach NetworkTechnologyOptions as technology>
											<option value="<tmpl_var technology.val>" <tmpl_if IN_SET(NetworksPreferredTechnology, technology.val)>selected="selected"</tmpl_if> ><tmpl_var technology.label></option>
										</tmpl_foreach>
									</select>
								<tmpl_else>
									<div class="col textCol">
										<tmpl_foreach NetworkTechnologyOptions as technology>
											<tmpl_var technology.label>
										</tmpl_foreach>
									</div>
								</tmpl_if>
							</div>
						</div>
					</div>
				</tmpl_if>
				<tmpl_if MnsPageLinkShow>
					<div class="row clearfix">
						<div class="col label">
						<tmpl_if showToolTip>
							<div class="tooltip tooltip-help" title="<tmpl_var _('webui', 'wwan_settings_mns_help_text')>">
								<tmpl_var _("webui", "webui_inline_help_icon")>
							</div>
						</tmpl_if>
							<tmpl_var _("webui", "wwan_settings_mns_label")>
						</div>
						<div class="col textCol" id="mnsStatus">
							<tmpl_var mnsStatus>
						</div>
						<tmpl_unless showToolTip>
							<div class="col help">
								<tmpl_var _("webui", "wwan_settings_mns_help_text")>
							</div>
						</tmpl_unless>
					</div>
					<div class="row clearfix">
						<div class="col label">
							</div>
							<tmpl_unless NetworksOtherAPNShow>
							<tmpl_if NetworksRevertToAutomaticShow>
								<button type="button" id="NetworksRevertToAutomatic" class="secondary"<tmpl_if NetworksRevertToAutomaticLocked> disabled="disabled"</tmpl_if><tmpl_unless mnsUsed> style="display:none;"</tmpl_unless><tmpl_if isLocked> disabled="disabled"</tmpl_if>>
									<tmpl_var _("webui", "wwan_settings_revert_mns_button")>
								</button>
							</tmpl_if>
							<tmpl_if NetworksSelectMNSShow>
								<button type="button" id="NetworksSelectMNS" class="secondary"<tmpl_if NetworksSelectMNSLocked> disabled="disabled"</tmpl_if><tmpl_if mnsUsed> disabled="disabled"</tmpl_if><tmpl_if isLocked> disabled="disabled"</tmpl_if>>
									<tmpl_var _("webui", "wwan_settings_mns_button")>
								</button>
							</tmpl_if>	
						<tmpl_else>
							<tmpl_if NetworksSelectMNSShow>
								<button type="button" id="NetworksSelectMNS" class="secondary"<tmpl_if NetworksSelectMNSLocked> disabled="disabled"</tmpl_if><tmpl_if mnsUsed> disabled="disabled"</tmpl_if><tmpl_if isLocked> disabled="disabled"</tmpl_if>>
									<tmpl_var _("webui", "wwan_settings_mns_button")>
								</button>
							</tmpl_if>
							<tmpl_if NetworksRevertToAutomaticShow>
								<button type="button" id="NetworksRevertToAutomatic" class="secondary"<tmpl_if NetworksRevertToAutomaticLocked> disabled="disabled"</tmpl_if><tmpl_unless mnsUsed> style="display:none;"</tmpl_unless><tmpl_if isLocked> disabled="disabled"</tmpl_if>>
									<tmpl_var _("webui", "wwan_settings_revert_mns_button")>
								</button>
							</tmpl_if>
						</tmpl_unless>
					</div>
				</tmpl_if>
			</div> <!-- Table -->
			<tmpl_unless apnProfileSettings>
				<tmpl_if ShowAdvancedSettingsToggleButton>
					<h2 id="advancedSettingsToggle" class="hideShowSection"><tmpl_var _("webui", "wwan_settings_advanced_settings_show")></h2>
				<tmpl_else>
					<h2 id="advancedSettingsHeading" class="sectionHead"><tmpl_var _("webui", "wwan_settings_advanced_settings_text")></h2>
				</tmpl_if>
				<div class="table" id="advancedSettings"<tmpl_if ShowAdvancedSettingsToggleButton> style="display:none;"</tmpl_if>>
					<tmpl_if NetworksFourGLTEAPNShow>
						<div class="row clearfix">
							<div class="col label">
								<label for="fourGLteApn"> <tmpl_var _("webui", "wwan_settings_fourg_lte_apn_label")></label>
							</div>
							<div class="col input">
								<input type="text" name="NetworksFourGLTEAPN" id="NetworksFourGLTEAPN"<tmpl_unless NetworksOtherAPNShow><tmpl_unless isSimReady> disabled="disabled"</tmpl_unless></tmpl_unless><tmpl_if NetworksFourGLTEAPNLocked> disabled="disabled"</tmpl_if><tmpl_if isLocked> disabled="disabled"</tmpl_if> maxlength="62" value="<tmpl_var NetworksFourGLTEAPN>"<tmpl_if NetworksFourGLTEAPNError> class="error"</tmpl_if> />
							</div>
							<tmpl_unless showPlaceHolderText>
								<div class="col help">
									<tmpl_udf GET_PO_TEXT("wwan_settings_fourg_lte_apn_help_text", gProductName)>
								</div>
							</tmpl_unless>
							<tmpl_if NetworksFourGLTEAPNError>
								<div class="col inline_error">
									<tmpl_var NetworksFourGLTEAPNError>
								</div>
							</tmpl_if>
						</div>
						<tmpl_if showPlaceHolderText>
							<div class="row clearfix">
								<tmpl_udf GET_PO_TEXT("wwan_settings_fourg_lte_apn_help_text", gProductName)>
							</div>
						</tmpl_if>
					</tmpl_if>

					<tmpl_if NetworksRestoreDefaultAPNShow>
						<div class="row clearfix">
							<div class="col label">
								<label></label>
							</div>
							<div class="col input">
								<button type="button" id="NetworksRestoreDefaultApn" class="secondary"<tmpl_if isLocked> disabled="disabled"</tmpl_if><tmpl_if NetworksRestoreDefaultAPNLocked> disabled="disabled"</tmpl_if>><tmpl_var _("webui", "wwan_settings_restore_apn_button")></button>
							</div>
						</div>
					</tmpl_if>

					<tmpl_if NetworksOtherAPNShow>
						<div class="row clearfix">
							<div class="col label">
								<label for="NetworksOtherAPN"> <tmpl_var _("webui", "wwan_settings_custom_apn_label")></label>

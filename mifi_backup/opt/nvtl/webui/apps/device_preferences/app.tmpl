<script type="text/javascript">
		var needConfirmMsg = 0;
		/* <![CDATA[ */
		$(function()
		{	
			$("input[id=usbModeChange]").val(0);
			// Bind and Initialize Toggle Button.
			if (jQuery.fn.slideButton) {
				$("#displaySettingsEnabled").slideButton("ajaxToggle", {
					checkUrl : "<tmpl_udf GET_URI('preferences_display_settings_enable')>",
					uncheckUrl : "<tmpl_udf GET_URI('preferences_display_settings_disable')>",
					data : {
						gSecureToken : "<tmpl_var gSecureToken>"
					}
				});
				
				$("#hardwareResetEnabled").slideButton("ajaxToggle", {
					checkUrl : "<tmpl_udf GET_URI('preferences_hardware_reset_enable')>",
					uncheckUrl : "<tmpl_udf GET_URI('preferences_hardware_reset_disable')>",
					data : {
						gSecureToken : "<tmpl_var gSecureToken>"
					}
				});
			}

			$("#usbMode").change(function()
			{
				$("input[id=usbModeChange]").val(1);
			});
		});
	</script>
<tmpl_if isHidden>
	<div class="notification app error">
		<tmpl_var _("webui", "webui_app_hidden_text")>
	</div>
<tmpl_else>
	<tmpl_if isLocked>
		<div class="notification app error">
			<tmpl_var _("webui", "webui_app_locked_text")>
		</div>
	</tmpl_if>

	<tmpl_if errorCount>
		<div class="notification app error">
			<div class="notification_inner" id="errorNotification">
				<p>
					<tmpl_var _("webui", "webui_save_code_failed_text")>
				</p>
			</div>
		</div>
	</tmpl_if>

	<tmpl_if gPostSuccess>
		<div class="notification app success">
			<div class="notification_inner" id="successNotification">
				<p>
					<tmpl_var _("webui", "webui_save_code_success_text")>
				</p>
			</div>
		</div>
	</tmpl_if>

	<div class="table">
		<div class="row clearfix">
			<div class="col-primary">
				<form id="preferences_form" method="POST" action="<tmpl_udf GET_URI('preferences')>">
					<div>
						<input type="hidden" name="gSecureToken" id="gSecureToken" value="<tmpl_var gSecureToken>" />
						<input type="hidden" name="usbModeChange" id="usbModeChange" value="<tmpl_var usbModeChange>" />
					</div>
					<fieldset id="general" class="first">
						<tmpl_if usbmodeShow>
							<div class="row clearfix">
								<div class="col label-teeny">
									<div class="col insignia">
										<tmpl_if showToolTip>
											<div class="tooltip tooltip-help" title="<tmpl_udf GET_PO_TEXT('preferences_usb_mode_help_text', gProductName)>">
												<tmpl_var _("webui", "webui_inline_help_icon")>
											</div>
										</tmpl_if>
									</div>
									<div class="col motto">
										<h4><label for="usbMode"><tmpl_var _("webui", "preferences_usb_mode_label")></label></h4>
									</div>
								</div>
								<div class="col input-textbox">
									<select name="usbMode" id="usbMode">
										<tmpl_include "usb_mode_options.tmpl">
									</select>
								</div>
								<tmpl_unless showToolTip>
									<div class="col help">
										<tmpl_udf GET_PO_TEXT("preferences_usb_mode_help_text", gProductName)>
									</div>
								</tmpl_unless>
							</div>
						</tmpl_if>
						<tmpl_if usbPromptShow>
							<div class="row clearfix">
								<div class="col label-teeny">
									<div class="col insignia">
										<tmpl_if showToolTip>
											<div class="tooltip tooltip-help" title="<tmpl_var _('webui', 'preferences_usb_connection_prompt_help_text')>">
												<tmpl_var _("webui", "webui_inline_help_icon")>
											</div>
										</tmpl_if>
									</div>
									<div class="col motto">
										<h4><label for="usbPromptEnabled"> <tmpl_var _("webui", "preferences_usb_connection_prompt_label")></label></h4>
									</div>
								</div>
								<div class="col input-textbox">
									<input type="checkbox" name="usbPromptEnabled" id="usbPromptEnabled"<tmpl_if IN_SET(usbPromptEnabled,"on")> checked="checked"</tmpl_if> />
								</div>
								<tmpl_unless showToolTip>
									<div class="col help">
										<tmpl_var _("webui", "preferences_usb_connection_prompt_help_text")>
									</div>
								</tmpl_unless>
							</div>
						</tmpl_if>

						<div class="row clearfix">
							<div class="col label-teeny">
								<div class="col insignia">
									<tmpl_if showToolTip>
										<div class="tooltip tooltip-help" title="<tmpl_var _('webui', 'preferences_turn_off_help_text')>">
											<tmpl_var _("webui", "webui_inline_help_icon")>
										</div>
									</tmpl_if>
								</div>
								<div class="col motto">
									<h4><label for="screenTimeout"><tmpl_var _("webui", "preferences_turnoff_diplay_label")></label></h4>
								</div>
							</div>
							<div class="col input-textbox">
								<select name="screenTimeout" id="screenTimeout">
									<tmpl_include "screen_timeout_options.tmpl">
								</select>
							</div>
							<tmpl_unless showToolTip>
								<div class="col help">
									<tmpl_var _("webui", "preferences_turn_off_help_text")>
								</div>
							</tmpl_unless>
						</div>

						<div class="row clearfix">
							<div class="col label-teeny">
								<div class="col insignia">
									<tmpl_if showToolTip>
										<div class="tooltip tooltip-help" title="<tmpl_udf GET_PO_TEXT('preferences_deep_sleep_help_text', gProductName)>">
											<tmpl_var _("webui", "webui_inline_help_icon")>
										</div>
									</tmpl_if>
								</div>
								<div class="col motto">
									<h4><label for="deepSleep"><tmpl_var _("webui", "preferences_deep_sleep_label")></label></h4>
								</div>
							</div>
							<div class="col input-textbox">
								<select name="deepSleep" id="deepSleep">
									<tmpl_include "deep_sleep_options.tmpl">
								</select>
							</div>
						</div>

						<tmpl_if autoshutdownShow>
						<div class="row clearfix">
							<div class="col label-teeny">
								<div class="col insignia">
									<tmpl_if showToolTip>
										<div class="tooltip tooltip-help" title="<tmpl_udf GET_PO_TEXT('preferences_auto_shutdown_help_text', gProductName)>">
											<tmpl_var _("webui", "webui_inline_help_icon")>
										</div>
									</tmpl_if>
								</div>
								<div class="col motto">
									<h4><label for="idleShutdownTime" id="lblIdleShutdownTime"><tmpl_var _("webui", "preferences_auto_shutdown_label")></label></h4>
								</div>
							</div>
							<div class="col input-textbox">
								<select name="idleShutdownTime" id="idleShutdownTime">
									<tmpl_include "shutdown_options.tmpl">
								</select>
							</div>
							<tmpl_unless showToolTip>
								<div class="col help" id="autoshutdownHelp">
									<tmpl_udf GET_PO_TEXT("preferences_auto_shutdown_help_text", gProductName)>
								</div>
							</tmpl_unless>
							<tmpl_if shutdownTimeError>
								<div class="col inline_error" id="shutdownTimeError">
									<tmpl_var shutdownTimeError>
								</div>
							</tmpl_if>
						</div>
						</tmpl_if>
					</fieldset>

					<tmpl_if universalChargingShow>
						<fieldset id="ucf" class="first">
							<!-- <div class="col-primary"> -->
							<h3 class="sectionHead"><tmpl_var _("webui", "preferences_universal_charging_sub_header_text")></h3>
							<p>
								<tmpl_udf GET_PO_TEXT("preferences_universal_charging_intro_text", gProductName)>
							</p>

							<div class="row clearfix">
								<div class="col label-teeny">
									<h4><label for="ucThreshold"><tmpl_udf GET_PO_TEXT("preferences_universal_charging_label", gProductName)></label></h4>
								</div>
								<div class="col input-textbox">
									<select name="ucThreshold" id="ucThreshold">
										<tmpl_include "uc_threshold_options.tmpl">
									</select>
								</div>
							</div>
							<div class="row clearfix">
								<div class="col label-teeny">
									<tmpl_if showToolTip>
										<div class="tooltip tooltip-help" title="<tmpl_var _('webui', 'preferences_universal_charging_status_help_text')>">
											<tmpl_var _("webui", "webui_inline_help_icon")>
										</div>
									</tmpl_if>
									<tmpl_var _("webui", "preferences_universal_charging_status_label")>
								</div>
								<div class="col input-textbox">
									<tmpl_var ucStatus>
								</div>
								<tmpl_unless showToolTip>
									<div class="col help">
										<tmpl_var _("webui", "preferences_universal_charging_status_help_text")>
									</div>
								</tmpl_unless>
							</div>
						</fieldset>
					</tmpl_if><!--universalChargingShow-->

					<div class="button_container clearfix ">
						<button type="submit" class="primary"<tmpl_if isLocked> disabled="disabled"</tmpl_if>>
							<tmpl_var _("webui", "webui_save_button")>
						</button>
					</div><!--button_container-->
			</div><!--col-primary-->

			<div class="col-secondary bg-color">
				<tmpl_if displaySettingsShow>
					<fieldset id="displaySettingsd" class="first">
						<div class="toggleSettingContainer" id="enableDisplaySettingsToggle">
							<div class="toggleSettingContainerInner">
								<div class="desc">
									<label for="displaySettingsEnabled">
										<div class="col insignia">
											<tmpl_if showToolTip>
												<div class="tooltip tooltip-help" title="<tmpl_udf GET_PO_TEXT('preferences_display_settings_toggle_text', gProductName)>">
													<tmpl_var _("webui", "webui_inline_help_icon")>
												</div>
											</tmpl_if>
										</div>
										<div class="col motto">
											<h4><tmpl_udf GET_PO_TEXT("preferences_display_settings_toggle_label", gProductName)></h4>
										</div>
									</label>
								</div>
								<div class="toggle">
									<input class="toggleSwitchCheckbox" type="checkbox" id="displaySettingsEnabled" name="displaySettingsEnabled"<tmpl_if IN_SET(displaySettingsEnabled, "true", "on", "1")> checked="checked"</tmpl_if> <tmpl_if isLocked>disabled="disabled"</tmpl_if> />
								</div>
							</div><!--/.toggleSettingContainerInner-->
						</div><!--/.toggleSettingContainer-->
					</fieldset>
				</tmpl_if><!--displaySettingsShow-->
				<fieldset id="hardwareResetSettings" class="first">
					<div class="toggleSettingContainer" id="enableDisplaySettingsToggle">
						<div class="toggleSettingContainerInner">
							<div class="desc">
								<label for="hardwareResetEnable">
									<div class="col insignia">
										<tmpl_if showToolTip>
											<div class="tooltip tooltip-help" title="<tmpl_var _('webui', 'preferences_hardware_reset_toggle_text')>">
												<tmpl_var _("webui", "webui_inline_help_icon")>
											</div>
										</tmpl_if>
									</div>
									<div class="col motto">
										<h4><tmpl_udf GET_PO_TEXT("preferences_hardware_reset_toggle_label", gProductName)></h4>
									</div>
								</label>
							</div>
							<div class="toggle">
								<input class="toggleSwitchCheckbox" type="checkbox" id="hardwareResetEnabled" name="hardwareResetEnabled"<tmpl_if IN_SET(hardwareResetEnabled, "true", "on", "1")> checked="checked"</tmpl_if> <tmpl_if isLocked>disabled="disabled"</tmpl_if> />
							</div>
						</div><!--/.toggleSettingContainerInner-->
					</div><!--/.toggleSettingContainer-->
				</fieldset>
			</div><!--col-secondary-->
		</div>
		
	</div><!--table-->
</tmpl_if>

<!DOCTYPE HTML>
<!-- paulirish.com/2008/conditional-stylesheets-vs-css-hacks-answer-neither/ -->
<!--[if lte IE 8]><html class="no-js ie8 oldie<tmpl_if IN_SET(gLangDirection, "rtl")> rtl</tmpl_if>" lang="<tmpl_var gHtmlLang>"<tmpl_if IN_SET(gLangDirection, "rtl")> dir="<tmpl_var gLangDirection>"</tmpl_if>><![endif]-->
<!--[if gt IE 8]><!--><html class="no-js<tmpl_if IN_SET(gLangDirection, "rtl")> rtl</tmpl_if>" lang="<tmpl_var gHtmlLang>"<tmpl_if IN_SET(gLangDirection, "rtl")> dir="<tmpl_var gLangDirection>"</tmpl_if>><!--<![endif]-->
	<head>
		<tmpl_include "meta.tmpl">
		<title><tmpl_include "admin_title.tmpl"></title>
		<tmpl_include "links.tmpl">
		<tmpl_include "scripts.tmpl">
	</head>
	<body id="<tmpl_var gPageId>" class="no_status_bar">
		<div id="container">
			<div id="container_inner">
				<tmpl_include "notification.tmpl">
				<div id="header">
					<div id="header_inner">
						<a id="carrier_logo" href="<tmpl_udf GET_URI('apps_home')>" title="<tmpl_udf GET_PO_TEXT('vzw_header_go_home_link', gProductName)>">
							<img src="/<tmpl_var gBaseAssetsPath>/images/logos/verizon_wireless.png" alt="Verizon Wireless" />
							<h3><tmpl_udf GET_PO_TEXT("status_bar_carrier_logo_sub_chunk_text", gProductName, gProductNumber)></h3>
						</a>
					</div>
				</div>

				<div id="main" role="main">
					<div id="main_inner">

						<tmpl_include "noscript.tmpl">

						<div id="app_container" class="page">
							<div id="app_inner">
								<div id="app_content">
									<tmpl_include "app.tmpl">
								</div>
							</div>
						</div> <!-- /#app_container -->

					</div>  <!-- /#main_inner -->
				</div> <!-- /#main -->

			</div> <!-- /#container_inner -->
			<div id="push"></div> <!-- for sticky footer -->
		</div> <!-- /#container -->

		<tmpl_include "footer.tmpl">
	</body>
</html>

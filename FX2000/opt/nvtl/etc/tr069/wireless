config wifi-device 'wlan0'
        option disabled '0'
	option hwmode '11ng'
	option htmode 'HT20'

config wifi-iface 'primary2g'
	option device 'wlan0'
	option network 'lan'
	option mode 'ap'
	option disabled '0'

config wifi-iface 'guest2g'
	option device 'wlan0'
	option network 'lan'
	option mode 'ap'
	option disabled '1'

config wifi-device 'wlan1'
	option disabled '0'
	option hwmode '11ac'
	option htmode 'HT80'

config wifi-iface 'primary5g'
	option device 'wlan1'
	option network 'lan'
	option mode 'ap'
	option disabled '0'

config wifi-iface 'guest5g'
	option device 'wlan1'
	option network 'lan'
	option mode 'ap'
	option disabled '1'


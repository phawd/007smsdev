<Config>
    <modelString>FX2000</modelString>
    <bootupDelayForWwan>30</bootupDelayForWwan>
    <backend>
        <cloudStorage>
            <enabled>1</enabled>	
            <ddFileName>dl_desc</ddFileName>
            <ddFileSize>2097152</ddFileSize>
            <connectionTimeoutSec>20</connectionTimeoutSec>
            <disableCertVerify>1</disableCertVerify>
            <production>
                <url>
                    <alias>real</alias>
                    <value>https://fota.production.nvtl.mifiupdates.com</value>
                </url>
            </production>
            <test>
                <url>
                    <alias>real</alias>
                    <value>https://fota.test.nvtl.mifiupdates.com</value>
                </url>
                <url>
                    <alias>small</alias>
                    <value>https://fota.test.nvtl.mifiupdates.com</value>
                </url>
                <url>
                    <alias>large</alias>
                    <value>https://fota.test.nvtl.mifiupdates.com</value>
                </url>
                <url>
                    <alias>invalid</alias>
                    <value>https://fota.test.nvtl.mifiupdates.com</value>
                </url>
                <url>
                    <alias>integrity</alias>
                    <value>https://fota.test.nvtl.mifiupdates.com</value>
                </url>
                <url>
                    <alias>oversize</alias>
                    <value>https://fota.test.nvtl.mifiupdates.com</value>
                </url>
                <url>
                    <alias>real_down</alias>
                    <value>https://fota.test.nvtl.mifiupdates.com</value>
                </url>
                <url>
                    <alias>small_down</alias>
                    <value>https://fota.test.nvtl.mifiupdates.com</value>
                </url>
                <url>
                    <alias>large_down</alias>
                    <value>https://fota.test.nvtl.mifiupdates.com</value>
                </url>
            </test>
             <key>
                 <ddDecryption>/opt/nvtl/etc/nua/dd_key</ddDecryption>
                <resultEncryption>/opt/nvtl/etc/nua/result_key</resultEncryption>
             </key>		
        </cloudStorage>
    </backend>    
    <softwareUpdate>
        <retry>
    	    <maxRoamingRetries>1</maxRoamingRetries>
            <maxNoDataRetries>1</maxNoDataRetries>
            <maxReportRetries>3</maxReportRetries>
            <maxDownloadRetries>5</maxDownloadRetries>
            <shortRetryIntervalSec>30</shortRetryIntervalSec>
            <longRetryIntervalSec>3600</longRetryIntervalSec>
        </retry>
        <autoInstall>
            <minutesAfterDL>1440</minutesAfterDL>
            <InstallInLocalTime>
                <Enabled>1</Enabled>
                <InstallWindowOpen>02</InstallWindowOpen>
                <InstallWindowEnd>04</InstallWindowEnd>
            </InstallInLocalTime>
        </autoInstall>
        <supportPriOnlyUpdate>1</supportPriOnlyUpdate>
        <preInstallKickOffWaitSec>5</preInstallKickOffWaitSec>
        <pollIntervalDays>1</pollIntervalDays>
        <installDefer>1</installDefer>
        <installAtPowerDown>1</installAtPowerDown>
        <installAtPowerUpTriggerCount>1</installAtPowerUpTriggerCount>        
        <SUCheckCondition>
            <DenyIfRoaming>1</DenyIfRoaming>
            <DenyIfNoCoverage>1</DenyIfNoCoverage>
            <DenyIfNoDataCall>1</DenyIfNoDataCall>
        </SUCheckCondition>
        <BatteryCheck>
            <Enabled>0</Enabled>
            <MinThreshold>10</MinThreshold>
            <MonitorEnabled>1</MonitorEnabled>		
        </BatteryCheck>
    </softwareUpdate>	
</Config>


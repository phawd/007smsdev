	<SoundNotificationTypes>
		<SoundNotificationType id="B1">
			<Sound>
				<!-- OnDuration and OffDuration are in milli-secs -->
				<OnDuration>275</OnDuration>
				<OffDuration>100</OffDuration>
				<!-- Frequency is an ID defined in buzzer library. Current values are:
				0=585HZ, 1=1KH, 2=1.75KH, 3=2KH, 4=2.342KHZ, 5=3KHZ, 6=3.5KHZ, 7=4KHZ, 8=5.26KHZ, 9=6KHZ -->
				<Frequency>0</Frequency>
				<!-- Number of times to buzz -->
				<RepeatCount>1</RepeatCount>
			</Sound>
			<Sound>

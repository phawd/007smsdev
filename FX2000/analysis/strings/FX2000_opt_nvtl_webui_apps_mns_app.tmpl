                        </tmpl_if>			
		$("#cancelButton").on("click", function(){
			$('.ui-dialog-content').dialog('close');
		noneFoundRow = $("#networkListTable tbody tr").first().clone();
		if ("<tmpl_var scanStatus>" != "" && mnsStatus != "disabled") {
			if (mnsIsEnabledBool) {
				$("#beforeScanText").hide();
				$("#scanStatus").hide();
			getScanningStatus();

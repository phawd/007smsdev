	<script type="text/javascript">
		/* <![CDATA[ */
		$(function()
			$(document).ajaxSuccess(function(e, xhr, settings) {
				if((xhr.responseText) && (settings.dataType == "json")){
					try {
						var obj = jQuery.parseJSON(xhr.responseText || "null");
						if(typeof obj.statusData.statusBarSINR !== "undefined") {
							$("#internetStatusSINR").text(obj.statusData.statusBarSINR + "<tmpl_var _('webui', 'status_diagnostics_db_unit')>");
						if(typeof obj.statusData.statusBarSNR !== "undefined") {
							$("#internetStatusSNR").text(obj.statusData.statusBarSNR + "<tmpl_var _('webui', 'status_diagnostics_db_unit')>");
						if(typeof obj.statusData.statusBarBand !== "undefined") {
							$("#band").html(obj.statusData.statusBarBand);
						if(typeof obj.statusData.statusBarBandwidth !== "undefined") {
							$("#bandwidth").html(obj.statusData.statusBarBandwidth);
					} catch(e) {}
			<tmpl_if DiagnosticsFieldTestShow>
					<tmpl_unless DiagnosticsFieldTestLocked>
					$("#fieldTestBtn").on("click", function(){
						window.open("<tmpl_udf GET_URI('field_test_info')>");
					});
				</tmpl_unless>
			</tmpl_if>
		/* ]]> */
	</script>

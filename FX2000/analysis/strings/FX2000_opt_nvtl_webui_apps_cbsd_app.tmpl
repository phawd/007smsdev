			$("#cbsdUrlRow2").toggle(show);
			$("#cbsdVersionRow1").toggle(hide);
			$("#cbsdVersionRow2").toggle(show);
			$("#cbsdUseridRow1").toggle(hide);
			$("#cbsdUseridRow2").toggle(show);
			$("#cpiUseridRow1").toggle(hide);
			$("#cpiUseridRow2").toggle(show);
			$("#cpiNameRow1").toggle(hide);
			$("#cpiNameRow2").toggle(show);
			$("#cpiPrivateKeyFileRow1").toggle(hide);
			$("#cpiPrivateKeyFileRow2").toggle(show);
			$("#caCertificateFileRow1").toggle(hide);
			$("#caCertificateFileRow2").toggle(show);
			$("#subCaCertificateFileRow1").toggle(hide);
			$("#subCaCertificateFileRow2").toggle(show);
			$("#privateKeyFileRow1").toggle(hide);
			$("#privateKeyFileRow2").toggle(show);
			$("#certPasswordRow1").toggle(hide);
			$("#certPasswordRow2").toggle(show);
			$("#antennaAzimuthRow1").toggle(hide);
			$("#antennaAzimuthRow2").toggle(show);
			$("#antennaDowntiltRow1").toggle(hide);
			$("#antennaDowntiltRow2").toggle(show);
		/* ]]> */
	</script>

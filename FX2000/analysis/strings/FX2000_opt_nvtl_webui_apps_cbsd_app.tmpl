		function validateServerUrlOrIp (url) {
                         if(url[0] == '-') {
                                return false;
                        }
                        var regUrl = /^[A-Za-z0-9-]+(\.[A-Za-z0-9-]+)*(\.[A-Za-z]{2,})$/;
                        if(regUrl.test(url) == true) {
                                return true;
                        } else {
                                if (validateServerIp(url) == true ) {
                                        return true;
                                } else {
                                        return false;
                                }
                        }
                }
                function validateServerIp (ip) {
                        console.log("validateServerIp start");
                        if (/^(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$/.test(ip)) {
                                return true;
                        } else {
                                return false;
                        }
                }
		function getRegistrationStatus() {
			$.ajax({
                                url : "<tmpl_udf GET_URI('cbsd_get_registration_status')>",
                                type : "GET",
                                dataType : "json",
                                success : function(data) {
					if(data.regSetting == 0) {
						$("#registrationStatus").html("<tmpl_var _('webui', 'cbsd_registration_status_not_registered_text')>");
					} else {	
						if(data.regState == 1 && data.responseCode != 0) {
							$("#registrationStatus").html("<tmpl_var _('webui', 'cbsd_registration_status_failed_text')>");
						} else if(data.regState == 2 || data.regState == 3 || data.regState == 4) {
							$("#registrationStatus").html("<tmpl_var _('webui', 'cbsd_registration_status_start_text')>");
						} else if(data.regState == 5) {
							$("#registrationStatus").html("<tmpl_var _('webui', 'cbsd_registration_status_registered_text')>");
					if(data.responseCode != 0) {
						$("#responseRow").show();
						if(data.responseMessage != "") {
							$("#responseMessage").html(data["responseMessage"]);
						} else {
							$("#responseMessage").html(getResponseCodeStr(data.responseCode));
						$("#responseCode").html(data["responseCode"]);
					} else {
						$("#responseRow").hide();
					if(data.isRegistrationAllowed == 1) {
						if( $("#registrationStatus").text() == "<tmpl_var _('webui', 'cbsd_de_registration_progress_text')>" ||
						    $("#registrationStatus").text() == "<tmpl_var _('webui', 'cbsd_cancel_registration_progress_text')>") {
							$("#registerDeviceButton").prop("disabled", true);
							if(! $("#registrationStatus").hasClass("loading") ) {
								$("#registrationStatus").addClass("loading");
							}
						} else if($("#registrationStatus").text() == "<tmpl_var _('webui', 'cbsd_registration_status_start_text')>") {
							$("#registerDeviceButton").prop("disabled", false);
							if(! $("#registrationStatus").hasClass("loading") ) {
								$("#registrationStatus").addClass("loading");
							}
						} else {
							$("#registerDeviceButton").prop("disabled", false);
							if( $("#registrationStatus").hasClass("loading") ) {
								$("#registrationStatus").removeClass("loading");
							}
					showHideElements(data.regSetting);
					changeButtonText(data.regSetting, data.regState, data.responseCode);
				error : function(xhr, textStatus, errorThrown) {
					// If auth session has ended, force a new login with a fresh GET.
        				if( (xhr.status == 401) || (xhr.status == 403) || (xhr.status == 406) ) window.location.replace(window.location.href);
				complete : function(xhr, textStatus) {
                                        setTimeout(getRegistrationStatus, pollingTime);
                                }
		function getResponseCodeStr(responseCode) {
			var str;
			switch(responseCode) {
				case 0:
					str = "<tmpl_var _('webui', 'cbsd_response_code_success_text')>";
					break;
				case 100: 
					str = "<tmpl_var _('webui', 'cbsd_response_code_version_not_supported_text')>";
					break;
				case 101:
					str = "<tmpl_var _('webui', 'cbsd_response_code_blacklisted_text')>";
					break;
				case 102:
					str = "<tmpl_var _('webui', 'cbsd_response_code_missing_param_text')>";
					break;
				case 103:
					str = "<tmpl_var _('webui', 'cbsd_response_code_invalid_value_text')>";
					break;
				case 104:
					str = "<tmpl_var _('webui', 'cbsd_response_code_certificate_error_text')>";
					break;
				case 105:
					str = "<tmpl_var _('webui', 'cbsd_response_code_deregister_error_text')>";
					break;
				case 200:
					str = "<tmpl_var _('webui', 'cbsd_response_code_registration_pending_text')>";
					break;
				case 201:
					str = "<tmpl_var _('webui', 'cbsd_response_code_group_error_text')>";
					break;
				case 300:
					str = "<tmpl_var _('webui', 'cbsd_response_code_unsupported_spectrum_text')>";
					break;
				case 400:
					str = "<tmpl_var _('webui', 'cbsd_response_code_interference_text')>";
					break;
				case 401:
					str = "<tmpl_var _('webui', 'cbsd_response_code_grant_conflict_text')>";
					break;
				case 500:
					str = "<tmpl_var _('webui', 'cbsd_response_code_grant_terminated_text')>";
					break;
				case 501:
					str = "<tmpl_var _('webui', 'cbsd_response_code_grant_suspended_text')>";
					break;
				case 502:
					str = "<tmpl_var _('webui', 'cbsd_response_code_unsync_op_param_text')>";
					break;
				case -1:
					str = "<tmpl_var _('webui', 'cbsd_response_code_not_in_band48_text')>";
					break;
				case -2:
					str = "<tmpl_var _('webui', 'cbsd_response_code_registration_request_prepare_failed_text')>";
					break;
				case -3:
					str = "<tmpl_var _('webui', 'cbsd_response_code_registration_request_post_failed_text')>";
					break;
				case -4:
					str = "<tmpl_var _('webui', 'cbsd_response_code_specutrum_inquiry_prepare_failed_text')>";
					break;
				case -5:
					str = "<tmpl_var _('webui', 'cbsd_response_code_spectrum_inquiry_post_failed_text')>";
					break;
				case -6:
					str = "<tmpl_var _('webui', 'cbsd_response_code_grant_request_prepare_failed_text')>";
					break;
				case -7:
					str = "<tmpl_var _('webui', 'cbsd_response_code_grant_request_post_failed_text')>";
					break;
				case -8: 
					str = "<tmpl_var _('webui', 'cbsd_response_code_heartbeat_request_prepare_failed_text')>";
					break;
				case -9:
					str = "<tmpl_var _('webui', 'cbsd_response_code_heartbeat_request_post_failed_text')>";
					break;
				default:
					str = "<tmpl_var _('webui', 'cbsd_response_code_registration_request_prepare_failed_text')>";
					break;
			return str;
		function changeButtonText(regSetting, regState, responseCode) {
			if($("#registrationStatus").text().trim() == '<tmpl_var _("webui", "cbsd_registration_status_start_text")>') {
				$("#registerDeviceButton").text("<tmpl_var _('webui', 'cbsd_de_register_button_text')>");
			} else 	if(regSetting == 0) {
				$("#registerDeviceButton").text("<tmpl_var _('webui', 'cbsd_register_device_button_text')>");
			} else {
				if(regState == 1) {
					if(responseCode != 0) {
						$("#registerDeviceButton").text("<tmpl_var _('webui', 'cbsd_cancel_registration_button_text')>");
				} else if(regState == 2 || regState == 3 || regState == 4) {
					$("#registerDeviceButton").text("<tmpl_var _('webui', 'cbsd_register_device_button_text')>");
				} else if(regState == 5) {
					$("#registerDeviceButton").text("<tmpl_var _('webui', 'cbsd_de_register_button_text')>");
		function showHideElements(regSetting) {
			var show = !regSetting;
			var hide = !show;
			$("#saveSasInfo").toggle(show);
			$("#saveCpiInfo").toggle(show);
			$("#saveCertInfo").toggle(show);
